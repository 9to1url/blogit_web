<div class="well">
  <ul class="nav">
    <li class="nav-header post-title">
      <a href="<%= post_path(@conn, :index) %>">Latest posts</a>
    </li>
    <%= for post <- @posts do %>
      <li class="postLink"><a href="<%= post_path(@conn, :show, post.name) %>"><%= post.title %></a></li>
    <% end %>
  </ul>
</div>
